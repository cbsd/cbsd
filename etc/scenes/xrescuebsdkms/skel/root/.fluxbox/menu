[begin] (Fluxbox)
[encoding] {UTF-8}
[exec]	(Terminal)	{urxvt}
[exec]	(MC)		{urxvt -e mc}
[separator]
[exec]	(screen)	{urxvt -e screen}
[exec]	(ViM)		{gvim}
[exec]	(xcalc)		{xcalc}
[exec]	(Opera)		{opera}
[exec]	(trafshow)	{urxvt -e trafshow}
[separator]
[exec]	(FreeBSD HandBook)	{opera /usr/local/share/doc/en_US.ISO8859-1/books/handbook/book.html}
[exec]	(CBSD Homepage)		{opera http://www.bsdstore.ru/}
[separator]
[exit] (Exit Fluxbox)
[endencoding]
[end]
