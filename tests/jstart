#!/bin/sh

$jname="test"

oneTimeSetUp() {
    if cbsd jstatus jname="${jname}"; then
		cbsd jcreate runasap=0 jname="${jname}"
	fi
}

setUp() {
    cbsd jstop jname="${jname}"
}

test_jstart(){
    cbsd jstart jname="${jname}"
    test=$(jexec ${jname} whoami)
    assertEqual "${test}" "root"
}


. shunit2